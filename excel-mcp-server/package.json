{
  "name": "@sbraind/excel-mcp-server",
  "version": "2.0.0",
  "description": "MCP Server for Excel file operations with 34 tools using ExcelJS",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "excel-mcp-server": "dist/index.js"
  },
  "scripts": {
